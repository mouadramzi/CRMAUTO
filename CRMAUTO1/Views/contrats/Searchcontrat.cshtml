@model IEnumerable<CRMAUTO.Models.contrat>

@{
    ViewBag.Title = "Contrat est : ";
}
<form action="#" method="get" class="">
    <div class="input-">
        CIN :    <input type="text" name="cin" class="" placeholder="CIN..."> Id contrat: <input type="text" name="ctt" class="" placeholder="Id contrat...">

        <span class="">
            <button type="submit" id="button2" value="Rechercher" formaction='@Url.Action("searchcontrat", "contrats")'>Rechercher</button>


        </span>
    </div>
</form>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.idvoiture)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.clientcin)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.datecontrat)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.kmdepart)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.idvoiture)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.clientcin)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.datecontrat)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.kmdepart)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.idcontrat }) |
                @Html.ActionLink("Details", "Details", new { id = item.idcontrat })
            </td>
        </tr>
    }

</table>
